<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <OutputType>Library</OutputType>
    <RootNamespace>RLC.FunctionalTypes</RootNamespace>
    <PackageId>RLC.FunctionalTypes</PackageId>
    <Title>FunctionalTypes</Title>
    <Authors>doomchild</Authors>
    <Description>Functional types for C#</Description>
    <Copyright>Copyright (c) 2022 Lee Crabtree</Copyright>
    <RepositoryUrl>https://github.com/doomchild/functional-types/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
